package kube

import "encoding/yaml"

k: ConfigMap: "hass-config": data: "configuration.yaml": """
	homeassistant:
	  name: Home
	  latitude: 59.382683
	  longitude: 18.005288
	  elevation: 10
	  unit_system: metric
	  time_zone: Europe/Stockholm
	  customize: !include customize.yaml

	group: !include groups.yaml
	automation: !include automations.yaml
	script: !include scripts.yaml
	scene: !include scenes.yaml

	logger:
	  default: info

	frontend:
	  themes: !include_dir_merge_named themes

	logbook:
	history:
	map:
	person:
	ssdp:
	sun:
	system_health:
	updater:
	zeroconf:
	discovery:
	config:
	mobile_app:

	tts:
	- platform: google_translate

	recorder:
	  db_url: !env_var DB_URL

	tv4_play:
	speedtestdotnet:

	google_assistant:
	  project_id: hass-dke
	  service_account: !include hass-dke-8e86dc9cd8ce.json
	  report_state: true
	  exposed_domains:
	  - switch
	  - light

	\(yaml.Marshal(conf))
	"""

let conf = {

	sensor: {}

	binary_sensor: [{
		platform: "workday"
		country:  "SE"
	}]

	input_boolean: kitchen_auto_mode: name: "Automatiskt läge i Köket"

	plant: {
		for i in ["xiaomi_hhccjcy01_48_63", "xiaomi_hhccjcy01_48_a6"] {
			"plant_\(i)": {
				min_moisture: 20
				sensors: {
					moisture:     "sensor.\(i)_moisture"
					temperature:  "sensor.\(i)_temperature"
					conductivity: "sensor.\(i)_soil_conductivity"
					brightness:   "sensor.\(i)_illuminance"
				}
			}
		}
	}
}

k: SealedSecret: "zwave-network-key": spec: encryptedData: key: ""

k: SealedSecret: "hass-gcp-credential-json": spec: encryptedData: "hass-dke-8e86dc9cd8ce.json": ""

k: SealedSecret: "hass-postgres-credentials": spec: encryptedData: {
	POSTGRESS_PASS: ""
	POSTGRES_USER:  ""
}
